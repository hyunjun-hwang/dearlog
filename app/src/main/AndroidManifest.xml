<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <!-- 인터넷 권한 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Dearlog"
        tools:targetApi="31"
        android:usesCleartextTraffic="true">

        <!-- 시작 액티비티 -->
        <activity
            android:name=".activity.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- 메인 액티비티 -->
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.SettingsActivity" />
        <activity android:name=".activity.WriteDiaryActivity" />
        <activity android:name=".activity.EmotionSummaryActivity" />
        <activity android:name=".activity.CalendarActivity" />
        <activity android:name=".activity.DiaryActivity" />
        <activity android:name=".activity.DiaryListActivity" />
        <activity android:name=".activity.FindIdActivity" />
        <activity android:name=".activity.FindPasswordActivity" />
    </application>

</manifest>